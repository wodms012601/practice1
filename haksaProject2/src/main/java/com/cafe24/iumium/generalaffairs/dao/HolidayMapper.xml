<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cafe24.iumium.generalaffairs.dao.HolidayMapper">

	<!-- 휴일 리스트 -->
    <select id="selectHolidayList"  resultType="com.cafe24.iumium.generalaffairs.dto.HolidayDto">
		SELECT holiday_name AS holidayName
					,holiday_start_day AS holidayStartDay
					,holiday_end_day AS holidayEndDay
		FROM ilban_holiday
    </select>
</mapper>